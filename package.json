{
    "name": "zce-pages",
    "version": "0.1.0",
    "description": "static web app workflow",
    "keywords": [
        "zce-pages",
        "zce"
    ],
    "repository": "luoluo/zce-pages",
    "license": "MIT",
    "author": " <limingxianweb@163.com> (https://luoluodeshijie/zce-pages)",
    "main": "lib/index.js",
    "bin": "bin/zce-pages.js",
    "files": [
        "lib",
        "bin"
    ],
    "scripts": {
        "lint": "standard --fix",
        "prepare": "npm run lint"
    },
    "renovate": {
        "extends": [
            "zce"
        ]
    },
    "dependencies": {
        "@babel/core": "^7.13.14",
        "@babel/plugin-proposal-decorators": "^7.13.15",
        "@babel/preset-env": "^7.13.12",
        "browser-sync": "^2.26.14",
        "del": "^6.0.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-htmlmin": "^5.0.1",
        "gulp-if": "^3.0.0",
        "gulp-imagemin": "^7.1.0",
        "gulp-load-plugins": "^2.0.6",
        "gulp-sass": "^4.1.0",
        "gulp-swig": "^0.9.1",
        "gulp-uglify": "^3.0.2",
        "gulp-useref": "^5.0.0"
    },
    "devDependencies": {
        "standard": "16.0.3"
    },
    "babel": {
        "plugins": [
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ]
        ],
        "presets": [
            "react-app"
        ]
    },
    "engines": {
        "node": ">=10"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    }
}
